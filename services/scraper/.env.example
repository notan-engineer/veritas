# Veritas Scraper Service - Environment Variables Template
# Copy this file to .env and update with your actual values

# Database connection
# For local development, use your local PostgreSQL credentials
# For Railway deployment, this will be auto-populated
DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@localhost:5432/veritas_local

# Environment
NODE_ENV=development

# Server port
PORT=3001

# Debug settings
DEBUG=true
LOG_LEVEL=debug
